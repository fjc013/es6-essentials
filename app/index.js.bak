// Use spread to pass function args
function print(a, b, c) {
  console.log(a, b, c);
}

let arr3 = [100, 200, 300];
print(...arr3);
// Another way
function printMe(...z) {
  console.log(z);
}
printMe(1000, 2000, 3000);